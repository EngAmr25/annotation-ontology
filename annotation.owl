<?xml version="1.0" encoding="UTF-8"?>

<!-- Created by Paolo Ciccarese http://www.paolociccarese.info on April 12th, 2010 -->

<!DOCTYPE rdf:RDF [
    <!-- Other namespaces -->
    <!ENTITY xsd  "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdf  "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY owl  "http://www.w3.org/2002/07/owl#" >
    <!ENTITY dct  "http://dublincore.org/documents/dcmi-terms/" >
    <!ENTITY ans  "http://www.w3.org/2000/10/annotation-ns#" >
 
    <!ENTITY foaf "http://xmlns.com/foaf/0.1/">
    <!ENTITY pav "http://swan.mindinformatics.org/ontologies/1.2/pav/">
    
    <!-- Main namespace -->
    <!ENTITY agents "http://purl.org/swan/annotation/" >
    
    <!-- Main files -->
    <!ENTITY local "file:///Users/paolociccarese/Websites/Mindinformatics/ontologies/1.2/agents.owl">
    <!ENTITY path "http://purl.org/swan/annotation/annotation-1_0.owl">
    <!ENTITY file "&path;">  
  	
    <!-- Imported files -->
    <!-- Local files to be removed before release -->
    <!ENTITY foaf-local "file:///Users/paolociccarese/Websites/Mindinformatics/ontologies/1.2/foaf-essential.owl">
    <!ENTITY pav-local "file:///Users/paolociccarese/Websites/Mindinformatics/ontologies/1.2/pav.owl">
  	
    <!ENTITY foaf-path "http://swan.mindinformatics.org/ontologies/1.2/foaf-essential.owl">
    <!ENTITY pav-path "http://swan.mindinformatics.org/ontologies/1.2/pav.owl">
  	
    <!ENTITY foaf-file "&foaf-path;">
    <!ENTITY pav-file "&pav-path;">
]>

<rdf:RDF
    xml:base="&agents;"
    xmlns:pav="&pav;"
    xmlns:rdfs="&rdfs;"
    xmlns:owl="&owl;"
    xmlns:rdf="&rdf;"
    xmlns:xsd="&xsd;"
    xmlns:dct="&dct;"
    xmlns:foaf="&foaf;"
    xmlns:ans="&ans;">
    
    <owl:Ontology rdf:about="&file;">
        <rdf:type rdf:resource="&owl;Ontology"/>
        <rdfs:label rdf:datatype="&xsd;string">Annotation Ontology v. 1.0</rdfs:label>

    	<owl:imports rdf:resource="&foaf-file;"/>
    	<owl:imports rdf:resource="&pav-file;"/>

    	<dct:language>en</dct:language>
    	<dct:title xml:lang="en">Annotation Ontology</dct:title>
    	<dct:creator rdf:resource="http://www.paolociccarese.info/"/>
    	<dct:publisher rdf:resource="http://www.mindinformatics.org"/>
    	<dct:contributor rdf:datatype="&xsd;string">Paolo Ciccarese</dct:contributor>
    	<dct:contributor rdf:datatype="&xsd;string">Marco Ocana</dct:contributor>
        <dct:created rdf:datatype="&xsd;string">April 12, 2010</dct:created>
        <dct:format rdf:datatype="&xsd;string">rdf/xml</dct:format>    
    </owl:Ontology> 
  
    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
     
    <rdf:Description rdf:about="&dct;title">
        <rdf:type rdf:resource="&owl;AnnotationProperty"/>
    </rdf:Description>
    <rdf:Description rdf:about="&dct;created">
        <rdf:type rdf:resource="&owl;AnnotationProperty"/>
    </rdf:Description>
    <rdf:Description rdf:about="&dct;creator">
        <rdf:type rdf:resource="&owl;AnnotationProperty"/>
    </rdf:Description>
    <rdf:Description rdf:about="&dct;format">
        <rdf:type rdf:resource="&owl;AnnotationProperty"/>
    </rdf:Description>
    <rdf:Description rdf:about="&dct;language">
        <rdf:type rdf:resource="&owl;AnnotationProperty"/>
    </rdf:Description>
    <rdf:Description rdf:about="&dct;publisher">
        <rdf:type rdf:resource="&owl;AnnotationProperty"/>
    </rdf:Description>
    <rdf:Description rdf:about="&dct;contributor">
        <rdf:type rdf:resource="&owl;AnnotationProperty"/>
    </rdf:Description>      

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Ontology Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
  	
    <owl:Class rdf:about="Annotation" rdfs:label="(ao) Annotation">
        <owl:disjointWith rdf:resource="AnnotationCuration"/>
        <owl:disjointWith rdf:resource="Selector"/>
        <owl:disjointWith rdf:resource="AnnotationSet"/>
    	
        <rdfs:comment rdf:datatype="&xsd;string">
        </rdfs:comment>
        
        <!-- created by -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pav;createdBy"/>
                <owl:allValuesFrom>
                    <owl:Class rdf:about="&foaf;Agent"/>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>                <owl:onProperty rdf:resource="&pav;createdBy"/>                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>            </owl:Restriction>
        </rdfs:subClassOf>	
		
        <!-- created on -->
        <rdfs:subClassOf>
            <owl:Restriction>                <owl:onProperty rdf:resource="&pav;createdOn"/>                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>            </owl:Restriction>
        </rdfs:subClassOf>	
        
        <!-- annotates -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ans;annotates"/>
                <owl:allValuesFrom>
                    <owl:Class rdf:about="&foaf;Document"/>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ans;annotates"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>	
        
        <!-- selector -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="selector"/>
                <owl:allValuesFrom>
                    <owl:Class rdf:about="Selector"/>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="selector"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        
        <!-- has topic -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="hasTopic"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>	
    </owl:Class>
  	
    <owl:Class rdf:about="Selector" rdfs:label="(ao) Selector">
        <owl:disjointWith rdf:resource="Annotation"/>
        <owl:disjointWith rdf:resource="AnnotationCuration"/>
        <owl:disjointWith rdf:resource="AnnotationSet"/>
    
        <rdfs:comment rdf:datatype="&xsd;string">
        </rdfs:comment>
        
        <!-- source document -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="onDocument"/>
                <owl:allValuesFrom>
                    <owl:Class rdf:about="&foaf;Document"/>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="onDocument"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>	
    </owl:Class>
  	
    <owl:Class rdf:about="AnnotationCuration" rdfs:label="(ao) AnnotationCuration">
        <owl:disjointWith rdf:resource="Annotation"/>
        <owl:disjointWith rdf:resource="Selector"/> 
        <owl:disjointWith rdf:resource="AnnotationSet"/>   
    
        <rdfs:comment rdf:datatype="&xsd;string">
        </rdfs:comment>
  		
        <!-- created by -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pav;createdBy"/>
                <owl:allValuesFrom>
                    <owl:Class rdf:about="&foaf;Agent"/>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>                <owl:onProperty rdf:resource="&pav;createdBy"/>                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>            </owl:Restriction>
        </rdfs:subClassOf>	
		
        <!-- created on -->
        <rdfs:subClassOf>
            <owl:Restriction>                <owl:onProperty rdf:resource="&pav;createdOn"/>                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>            </owl:Restriction>
        </rdfs:subClassOf>
        
        <!-- curates (exactly one annotation) -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="curates"/>
                <owl:allValuesFrom>
                    <owl:Class rdf:about="Annotation"/>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="curates"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="curates"/>
                <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        
        <!-- status -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="status"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>	
    </owl:Class>
    
    <owl:Class rdf:about="AnnotationSet" rdfs:label="(ao) AnnotationCuration">
        <owl:disjointWith rdf:resource="Annotation"/>
        <owl:disjointWith rdf:resource="AnnotationCuration"/>
        <owl:disjointWith rdf:resource="Selector"/>    
    
        <rdfs:comment rdf:datatype="&xsd;string">
        </rdfs:comment>
  		
        <!-- created by -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pav;createdBy"/>
                <owl:allValuesFrom>
                    <owl:Class rdf:about="&foaf;Agent"/>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pav;createdBy"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>	
		
        <!-- created on -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pav;createdOn"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>

        <!-- has topic -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="hasTopic"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>	
        
        <!-- source document -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="onDocument"/>
                <owl:allValuesFrom>
                    <owl:Class rdf:about="&foaf;Document"/>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="onDocument"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>	
    </owl:Class>

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation Ontology Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
    <owl:ObjectProperty rdf:about="onDocument">
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="hasTopic">
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="status">
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="curates">
        <owl:inverseOf rdf:resource="isCuratedBy" />
    </owl:ObjectProperty>

</rdf:RDF>